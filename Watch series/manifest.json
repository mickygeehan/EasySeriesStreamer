{
  "name": "The popup",
  "version": "1.0",
    "manifest_version": 2,
  "background" : {
    "scripts" : ["js/openNewTab.js"]
  },
  "permissions": [
    "http://*/*",
      "http://*/",
    "https://*/*",
    "https://www.google.com/*",
    "http://www.google.com/*"
    ],
  "browser_action": {
    "default_icon": "logo.png"
  }
}
